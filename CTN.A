local noclip = false
local player = game.Players.LocalPlayer
local char = player.Character or player.CharacterAdded:Wait()

-- GUI Setup
local gui = Instance.new("ScreenGui", player:WaitForChild("PlayerGui"))
gui.ResetOnSpawn = false

local frame = Instance.new("Frame", gui)
frame.Size = UDim2.new(0, 130, 0, 40)
frame.Position = UDim2.new(0.4, 0, 0.3, 0)
frame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)

local button = Instance.new("TextButton", frame)
button.Size = UDim2.new(1, 0, 0.5, 0)
button.Position = UDim2.new(0, 0, 0, 0)
button.Text = "CNA: OFF"
button.TextColor3 = Color3.fromRGB(255, 255, 255)
button.BackgroundColor3 = Color3.fromRGB(50, 50, 50)

-- Movement Buttons
local directions = {"↑", "↓", "←", "→"}
local offsets = {
    Vector2.new(0, -10),
    Vector2.new(0, 10),
    Vector2.new(-10, 0),
    Vector2.new(10, 0)
}

for i = 1, 4 do
    local dirBtn = Instance.new("TextButton", frame)
    dirBtn.Size = UDim2.new(0.25, 0, 0.5, 0)
    dirBtn.Position = UDim2.new((i - 1) * 0.25, 0, 0.5, 0)
    dirBtn.Text = directions[i]
    dirBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
    dirBtn.BackgroundColor3 = Color3.fromRGB(40, 40, 40)

    dirBtn.MouseButton1Click:Connect(function()
        local pos = frame.Position
        local offset = offsets[i]
        frame.Position = UDim2.new(pos.X.Scale, pos.X.Offset + offset.X, pos.Y.Scale, pos.Y.Offset + offset.Y)
    end)
end

-- Toggle CNA (NoClip)
button.MouseButton1Click:Connect(function()
    noclip = not noclip
    button.Text = "CNA: " .. (noclip and "ON" or "OFF")
end)

-- CNA logic (skip HumanoidRootPart)
game:GetService("RunService").Stepped:Connect(function()
    if noclip and char then
        for _, part in pairs(char:GetDescendants()) do
            if part:IsA("BasePart") and part.CanCollide and part.Name ~= "HumanoidRootPart" then
                part.CanCollide = false
            end
        end
    end
end)
